{
  "name": "chatty",
  "productName": "Chatty",
  "version": "1.3.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
    "start-local": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "build": {
    "appId": "com.info.chatty",
    "productName": "Chatty",
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "_static/**/*",
      "node_modules/**/*",
      "RELEASE_NOTES.md",
      "package.json"
    ],
    "mac": {
      "icon": "assets/icons/icon.icns",
      "target": [
        "dmg"
      ]
    },
    "dmg": {
      "sign": false
    },
    "generateUpdatesFilesForAllChannels": false,
    "asar": true
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.2",
    "@tabler/icons-react": "^2.9.0",
    "@types/adm-zip": "^0.5.0",
    "adm-zip": "^0.5.10",
    "asar": "^3.2.0",
    "child_process": "^1.0.2",
    "dist": "^0.1.2",
    "eventsource-parser": "^0.1.0",
    "express": "^4.18.2",
    "html-to-image": "^1.11.11",
    "i18next": "^22.4.13",
    "next": "^13.4.9",
    "next-i18next": "^13.2.2",
    "npm": "^9.8.0",
    "openai": "^3.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.2.0",
    "react-markdown": "^8.0.7",
    "react-modal": "^3.16.1",
    "react-query": "^3.39.3",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-mathjax": "^4.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "tree-kill": "^1.2.2",
    "uuid": "^9.0.0",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@mozilla/readability": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-modal": "^3.16.0",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/uuid": "^9.0.1",
    "@vitest/coverage-c8": "^0.29.7",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "endent": "^2.1.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "gpt-3-encoder": "^1.1.4",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "tailwindcss": "^3.2.7",
    "typescript": "4.9.5",
    "vitest": "^0.29.7"
  }
}
